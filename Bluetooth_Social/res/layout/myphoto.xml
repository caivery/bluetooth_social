<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@color/sex_select_color" android:orientation="vertical"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:weightSum="1">

	<FrameLayout android:id="@+id/FrameLayout01"
	android:layout_width="fill_parent" android:layout_height="75px"
	xmlns:android="http://schemas.android.com/apk/res/android"
	>
	<TextView 
		android:id="@+id/myphoto_title_text" 
		android:layout_height="45dip" 
		android:textColor="@color/white"
		android:textSize="19dip"
		android:text="@string/myphoto_text"
		android:layout_width="fill_parent" 
		android:background="@drawable/title_bar"
	 	android:gravity="center"/>
	<Button android:layout_gravity="left" android:textColor="@color/white" android:id="@+id/myphoto_bt_left" android:layout_height="40dip" android:textSize="14dip" android:background="@drawable/title_back_xml" android:layout_width="63dip" android:text="@string/tittle_back"></Button>
	
</FrameLayout>
	
	<edu.minggo.chat.util.BounceListView 
		android:scrollbars="none"
		android:layout_height="wrap_content" 
		android:divider="@color/white" 
		android:background="@color/white" 
		android:id="@+id/myphoto_listview" 
		android:dividerHeight="1.0dip" 
		android:layout_width="fill_parent" 
		android:cacheColorHint="#00000000" 
		android:layout_alignParentLeft="true" android:layout_alignParentRight="true" />
</LinearLayout>
